<p id="notice"><%= notice %></p>

<h1>Listing Warehouse Receipts</h1>

<table>
  <thead>
    <tr>
      <th>Warehouse</th>
      <th>Supplier order</th>
      <th>Total excl</th>
      <th>Total incl</th>
      <th>Reduce price</th>
      <th>Reduce percent</th>
      <th>Total reduced</th>
      <th>Status</th>
      <th>Created</th>
      <th>Creator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @warehouse_receipts.each do |warehouse_receipt| %>
      <tr>
        <td><%= warehouse_receipt.warehouse %></td>
        <td><%= warehouse_receipt.supplier_order %></td>
        <td><%= warehouse_receipt.total_excl %></td>
        <td><%= warehouse_receipt.total_incl %></td>
        <td><%= warehouse_receipt.reduce_price %></td>
        <td><%= warehouse_receipt.reduce_percent %></td>
        <td><%= warehouse_receipt.total_reduced %></td>
        <td><%= warehouse_receipt.status %></td>
        <td><%= warehouse_receipt.created %></td>
        <td><%= warehouse_receipt.creator %></td>
        <td><%= link_to 'Show', warehouse_receipt %></td>
        <td><%= link_to 'Edit', edit_warehouse_receipt_path(warehouse_receipt) %></td>
        <td><%= link_to 'Destroy', warehouse_receipt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Warehouse receipt', new_warehouse_receipt_path %>
